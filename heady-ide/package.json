{
  "name": "heady-ide",
  "version": "1.0.0",
  "description": "HeadyAI-IDE â€” Model Provider & Arena Merge Engine",
  "main": "main.js",
  "author": "Heady Systems",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "build:win": "electron-builder --win",
    "build:all": "electron-builder --win --linux --mac"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "electron": "^35.7.5",
    "electron-builder": "^26.7.0"
  },
  "build": {
    "appId": "com.heady.ide",
    "productName": "HeadyAI-IDE",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "renderer.js",
      "index.html",
      "styles.css",
      "assets/**/*",
      "src/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "publisherName": "Heady Systems",
      "verifyUpdateCodeSignature": false,
      "sign": false
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.png",
      "category": "Development"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "assets/icon.icns",
      "category": "public.app-category.developer-tools"
    }
  }
}